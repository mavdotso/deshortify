{
    "name": "DeShortify",
    "description": "Remove YouTube Shorts from your subscriptions and feed",
    "version": "2.0",
    "manifest_version": 3,
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["script.js"]
        }
    ]
}
